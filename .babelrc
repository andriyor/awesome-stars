{
  "presets": [
    ["env", {
      "targets": {
        "browsers": "last 2 Chrome versions"
      },
      "useBuiltIns": true,
      "debug": false
    }],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-runtime"
  ],
  "ignore": ["node_modules"]
}
