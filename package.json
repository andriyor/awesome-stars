{
  "name": "awesome-stars",
  "version": "4.0.0",
  "description": "A Chrome extension that reveals star count of repositories on awesome list",
  "scripts": {
    "build": "webextension-toolbox build",
    "build-storybook": "build-storybook",
    "dev": "webextension-toolbox dev",
    "lint": "eslint app/**/*.ts",
    "precommit": "npm run prettier && npm run lint",
    "prettier": "prettier --write .storybook/**/*.js app/**/*.ts stories/**/*.ts stories/**/*.tsx",
    "storybook": "start-storybook -p 6006",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.3",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.8.10",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^5.16.0",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "prettier": "^1.16.4",
    "styled-components": "^4.2.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.3.4000",
    "webextension-polyfill-ts": "^0.8.9",
    "webextension-toolbox": "^3.0.0",
    "webpack-watched-glob-entries-plugin": "^2.1.4"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/henry40408/awesome-stars.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.0.0",
    "react": "^16.9.0"
  }
}
